{
  "name": "@hashgraph/mirror-rest",
  "version": "0.70.0-SNAPSHOT",
  "description": "Hedera Mirror Node REST API",
  "type": "module",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": ">=16.13.0 < 19"
  },
  "scripts": {
    "dev": "HEDERA_MIRROR_REST_LOG_LEVEL=trace nodemon --experimental-specifier-resolution=node server.js",
    "lint": "eslint --ignore-pattern node_modules/ --fix .",
    "postinstall": "node ./npm_scripts/postinstall.js",
    "start": "node --experimental-specifier-resolution=node server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --maxWorkers 2 --testPathPattern='__tests__/.*\\.test\\.js'"
  },
  "author": "Hedera Mirror Node Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@awaitjs/express": "^0.9.0",
    "@godaddy/terminus": "^4.11.2",
    "@hashgraph/proto": "^2.11.0",
    "asn1js": "^3.0.5",
    "aws-sdk": "^2.1248.0",
    "bigint-buffer": "^1.1.5",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "express-openapi-validator": "^4.13.8",
    "extend": "^3.0.2",
    "ip-anonymize": "^0.1.0",
    "js-yaml": "^4.1.0",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.7.0",
    "long": "^5.2.1",
    "mathjs": "^11.3.2",
    "mem": "^9.0.2",
    "parse-duration": "^1.0.2",
    "pg": "~8.8.0",
    "pg-range": "^1.1.2",
    "prom-client": "^13.1.0",
    "qs": "^6.11.0",
    "quick-lru": "^6.1.1",
    "rfc4648": "^1.5.2",
    "swagger-stats": "^0.99.2",
    "swagger-ui-express": "^4.5.0"
  },
  "bundledDependencies": [
    "@awaitjs/express",
    "@godaddy/terminus",
    "@hashgraph/proto",
    "asn1js",
    "aws-sdk",
    "bigint-buffer",
    "body-parser",
    "compression",
    "cors",
    "express",
    "express-http-context",
    "express-openapi-validator",
    "extend",
    "ip-anonymize",
    "js-yaml",
    "json-bigint",
    "lodash",
    "log4js",
    "long",
    "mathjs",
    "mem",
    "parse-duration",
    "pg",
    "pg-range",
    "prom-client",
    "qs",
    "quick-lru",
    "rfc4648",
    "swagger-stats",
    "swagger-ui-express"
  ],
  "devDependencies": {
    "app-root-path": "^3.1.0",
    "aws-sdk-mock": "^5.8.0",
    "axios": "^1.1.3",
    "codecov": "^3.8.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.2",
    "jest": "^29.2.2",
    "jest-circus": "^29.3.1",
    "jest-extended": "^3.1.0",
    "jest-junit": "^14.0.1",
    "jest-standard-reporter": "^2.0.0",
    "node-flywaydb": "^3.0.7",
    "nodemon": "^2.0.20",
    "pg-format": "^1.0.4",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rewire": "^6.0.0",
    "sinon": "^14.0.2",
    "supertest": "^6.3.1",
    "testcontainers": "^9.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/__tests__/"
    ],
    "globalSetup": "./__tests__/globalSetup.js",
    "maxWorkers": "50%",
    "reporters": [
      "jest-standard-reporter",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/jestSetup.js"
    ],
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner",
    "verbose": true
  },
  "jest-junit": {
    "outputDirectory": "target/jest-junit"
  },
  "overrides": {
    "json-schema": "0.4.0",
    "multer": "1.4.4-lts.1",
    "protobufjs": "6.11.3"
  },
  "prettier": {
    "bracketSpacing": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "baseUrlPath": "/api/v1"
}
