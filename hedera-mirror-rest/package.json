{
  "name": "@hashgraph/mirror-rest",
  "version": "0.53.0-SNAPSHOT",
  "description": "Hedera Mirror Node REST API",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": ">=16.13.0 < 17"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "integrationtest": "jest --testPathPattern='/__tests__/integration.test.js|/__tests__/service/.*\\.test\\.js' --collectCoverage=false",
    "lint": "eslint --ignore-pattern node_modules/ --fix .",
    "postinstall": "node ./npm_scripts/postinstall.js",
    "test": "jest --testPathPattern='__tests__/.*\\.test\\.js'"
  },
  "author": "Hedera Mirror Node Team",
  "comments": {
    "dependencies": "follow-redirects, node-fetch & json-schema are transitive dependencies but we explicitly mark them as a dependency to bring in a non-vulnerable version"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@awaitjs/express": "^0.9.0",
    "@godaddy/terminus": "^4.10.2",
    "@hashgraph/proto": "^2.1.5",
    "asn1js": "^2.2.0",
    "aws-sdk": "^2.1082.0",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-http-context": "^1.2.4",
    "express-openapi-validator": "^4.13.5",
    "extend": "^3.0.2",
    "follow-redirects": "^1.14.9",
    "ip-anonymize": "^0.1.0",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "long": "^5.2.0",
    "mathjs": "^10.4.0",
    "mem": "^8.1.1",
    "parse-duration": "^1.0.2",
    "pg": "~8.7.3",
    "pg-range": "^1.1.1",
    "prom-client": "^14.0.1",
    "qs": "^6.10.3",
    "quick-lru": "^5.1.1",
    "rfc4648": "^1.5.1",
    "swagger-stats": "^0.99.2",
    "swagger-ui-express": "^4.3.0"
  },
  "bundledDependencies": [
    "@awaitjs/express",
    "@godaddy/terminus",
    "@hashgraph/proto",
    "asn1js",
    "aws-sdk",
    "body-parser",
    "compression",
    "cors",
    "express",
    "express-http-context",
    "express-openapi-validator",
    "extend",
    "follow-redirects",
    "ip-anonymize",
    "js-yaml",
    "json-schema",
    "lodash",
    "log4js",
    "long",
    "mathjs",
    "mem",
    "parse-duration",
    "pg",
    "pg-range",
    "prom-client",
    "qs",
    "quick-lru",
    "rfc4648",
    "swagger-stats",
    "swagger-ui-express"
  ],
  "devDependencies": {
    "app-root-path": "^3.0.0",
    "aws-sdk-mock": "^5.6.2",
    "axios": "^0.26.0",
    "codecov": "^3.8.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-circus": "^27.4.6",
    "jest-extended": "^2.0.0",
    "jest-junit": "^13.0.0",
    "node-fetch": "^2.6.7",
    "mock-req-res": "^1.2.0",
    "node-flywaydb": "^3.0.7",
    "nodemon": "^2.0.15",
    "pg-format": "^1.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rewire": "^6.0.0",
    "sinon": "^13.0.1",
    "supertest": "^6.2.2",
    "testcontainers": "^8.2.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/__tests__/"
    ],
    "maxWorkers": "50%",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "./__tests__/jest.setup.js"
    ],
    "testEnvironment": "node",
    "testRunner": "jest-circus/runner",
    "verbose": true
  },
  "jest-junit": {
    "outputDirectory": "target/jest-junit"
  },
  "prettier": {
    "bracketSpacing": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "baseUrlPath": "/api/v1"
}
