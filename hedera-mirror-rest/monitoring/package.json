{
  "name": "@hashgraph/mirror-monitor",
  "version": "0.121.0-SNAPSHOT",
  "description": "Hedera Mirror Node Monitor",
  "type": "module",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": ">=20 < 21"
  },
  "scripts": {
    "dev": "nodemon --import=extensionless/register server.js",
