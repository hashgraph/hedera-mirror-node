  },
  "author": "Hedera Mirror Node Team",
  "license": "Apache-2.0",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "extend": "^3.0.2",
    "extensionless": "^1.9.9",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "mathjs": "^14.0.0",
    "parse-duration": "^1.1.1",
    "pretty-ms": "^9.2.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "jest-junit": "^16.0.0",
    "jest-standard-reporter": "^2.0.0"
  },
  "jest-junit": {
    "outputDirectory": "build/test-results/test",
    "outputName": "TEST-jest.xml"
  },
