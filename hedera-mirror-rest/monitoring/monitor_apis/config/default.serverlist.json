{
  "port": 3000,
  "servers": [
    {
      "baseUrl": "https://testnet.mirrornode.hedera.com",
      "name": "testnet"
    }
  ],
  "interval": 60,
  "retry": {
    "maxAttempts": 2,
    "minBackoff": 1000
  },
  "shard": 0,
  "timeout": 30,
  "account": {
    "enabled": true,
    "intervalMultiplier": 1,
    "limit": 10
  },
  "balance": {
    "enabled": true,
    "freshnessThreshold": 1000,
    "intervalMultiplier": 1,
    "limit": 10
  },
  "block": {
    "enabled": true,
    "freshnessThreshold": 1000,
    "intervalMultiplier": 1,
    "limit": 10
  },
  "network": {
    "enabled": true,
    "intervalMultiplier": 1
  },
  "stateproof": {
    "enabled": true,
    "intervalMultiplier": 10
  },
  "transaction": {
    "enabled": true,
    "freshnessThreshold": 50,
    "intervalMultiplier": 1,
    "limit": 10
  },
  "topic": {
    "enabled": true,
    "freshnessThreshold": 100,
    "intervalMultiplier": 1,
    "limit": 10,
    "topicId": ""
  },
  "token": {
    "enabled": false,
    "freshnessThreshold": 1000,
    "intervalMultiplier": 1,
    "tokenBalancesLimit": 10,
    "tokenId": "",
    "limit": 10
  }
}
