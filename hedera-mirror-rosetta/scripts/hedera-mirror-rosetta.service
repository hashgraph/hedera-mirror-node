[Unit]
After=syslog.target
Description=Hedera Mirror Rosetta API

[Service]
Environment=HEDERA_MIRROR_ROSETTA_API_CONFIG=/opt/hedera-mirror-rosetta/application.yml
ExecStart=/opt/lib/hedera-mirror-rosetta/hedera-mirror-rosetta
LimitNOFILE=65536
Restart=on-failure
RestartSec=1
SuccessExitStatus=143
Type=simple
WorkingDirectory=/opt/lib/hedera-mirror-rosetta

[Install]
WantedBy=multi-user.target
